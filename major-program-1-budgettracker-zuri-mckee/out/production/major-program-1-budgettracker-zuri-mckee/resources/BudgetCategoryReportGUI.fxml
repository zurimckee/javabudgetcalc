<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.12" xmlns:fx="http://javafx.com/fxml/1" fx:controller="BudgetCategoryReportController">
    <children>
        <HBox alignment="CENTER" prefHeight="35.0" prefWidth="600.0">
            <children>
                <Label text="Category Report">
                    <font>
                        <Font name="System Bold" size="16.0" />
                    </font>
                </Label>
            </children>
        </HBox>
        <HBox alignment="CENTER_LEFT" prefHeight="42.0" prefWidth="600.0">
            <children>
             <!--   <ComboBox fx:id="cmbCategory" onAction="#onComboClick" prefWidth="150.0" />  -->
            <ComboBox fx:id="cmbCategory2" onAction="#onComboClick2" prefWidth="150.0" />
            </children>
         <padding>
            <Insets left="10.0" />
         </padding>
        </HBox>

        <TextArea fx:id="txtReport" prefHeight="285.0" prefWidth="600.0" />
        <HBox alignment="CENTER" prefHeight="60.0" prefWidth="600.0">
            <Button fx:id="btnClose" mnemonicParsing="false" onAction="#onBtnCloseClick" text="Close" />
        </HBox>

    </children>
</VBox>
